<node name="/org/informfiction/garglk">
  <interface name="org.informfiction.garglk">

    <method name="SetInteractive">
      <arg type="b" name="interactive" direction="in" />
    </method>

    <method name="SetProtected">
      <arg type="b" name="protect" direction="in" />
    </method>

    <method name="FeedText">
      <arg type="s" name="text" direction="in" />
    </method>

    <method name="RequestWindow">
      <arg type="u" name="windowid" direction="out" />
    </method>

    <method name="SetMinimumSize">
      <arg type="u" name="x" direction="in" />
      <arg type="u" name="y" direction="in" />
    </method>

    <method name="GrabFocus"></method>

    <method name="Stop"></method>

    <signal name="CommandText">
      <arg type="s" name="text" />
    </signal>

    <signal name="ReplyText">
      <arg type="s" name="text" />
    </signal>

    <signal name="StoryTitle">
      <arg type="s" name="title" />
    </signal>

    <signal name="Ready"></signal>

    <signal name="Stopped"></signal>

  </interface>
</node>
